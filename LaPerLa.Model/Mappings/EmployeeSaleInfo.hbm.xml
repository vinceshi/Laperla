<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="LaPerLa.Model"
                   namespace="LaPerLa.Model">
  <class name="EmployeeSaleInfo">
    <id name="EmployeeSaleId">
      <generator class="identity" />
    </id>
    <property name="EmployeeId" />
    <property name="ShopId" />
    <property name="Week" />
    <property name="SaleType" />
    <property name="Sale" />
    <property name="Days" />
    <property name="Month" />
  </class>
</hibernate-mapping>